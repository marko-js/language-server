{
  "name": "@marko/language-server",
  "description": "Marko Language Server",
  "version": "1.1.6",
  "bin": {
    "marko-language-server": "./bin.js"
  },
  "bugs": "https://github.com/marko-js/language-server/issues/new?template=Bug_report.md",
  "dependencies": {
    "@babel/helper-validator-identifier": "^7.22.20",
    "@marko/language-tools": "^2.1.4",
    "@marko/babel-utils": "^6.3.1",
    "@marko/compiler": "^5.33.2",
    "@marko/translator-default": "^5.31.3",
    "htmljs-parser": "^5.5.0",
    "marko": "^5.31.10",
    "prettier": "^2.8.8",
    "prettier-plugin-marko": "^2.1.0",
    "relative-import-path": "^1.0.0",
    "strip-json-comments": "^3.1.1",
    "typescript": "^5.2.2",
    "vscode-css-languageservice": "^6.2.9",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11",
    "vscode-uri": "^3.0.7"
  },
  "devDependencies": {
    "@types/prettier": "^2.7.3",
    "tsx": "^3.13.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/__tests__",
    "!**/*.tsbuildinfo"
  ],
  "homepage": "https://github.com/marko-js/language-server/tree/main/packages/language-server/README.md",
  "keywords": [
    "autocomplete",
    "intellisense",
    "language",
    "marko",
    "server"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/language-server/tree/main/packages/language-server"
  },
  "scripts": {
    "build": "tsc -b && tsx build.mts",
    "test": "mocha './src/**/__tests__/*.test.ts'",
    "test:update": "mocha './src/**/__tests__/*.test.ts' --update"
  }
}
